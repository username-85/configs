    from root
/home/user/dev/sh/net/run

    from user

~/dev/sh/tmux_start_sessions.sh

docker run -t -i --rm --privileged --net=vlan100 local:stand
ip address flush dev eth0
ifconfig eth0 192.168.100.10 netmask 255.255.255.0 up
ifconfig eth0 inet6 add 2001:db8:abc8::2/64


docker run -t -i --rm --privileged --net=vlan200 local:stand
ip address flush dev eth0
ifconfig eth0 192.168.100.11 netmask 255.255.255.0 up
ifconfig eth0 inet6 add 2001:db8:abc8::3/64
